<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<display-name>Chinese Chess</display-name>
	<servlet>
		<servlet-name>cchess</servlet-name>
		<servlet-class>
			com.trendmicro.mobilelab.game.cchess.CChessServlet
		</servlet-class>
    </servlet>

	<servlet-mapping>
		<servlet-name>cchess</servlet-name>
		<url-pattern>/game/*</url-pattern>
	</servlet-mapping>
	
</web-app>
